package test;
import java.util.Scanner;
/*
 *        
*/

public  class demo {	
	public static void main(String[] args)   throws Exception
	{
		int[] m={4,6,9,1,3,8,6,3,0,56};
		
		int container =m[0];
		
		for(int i=0;i<m.length;i++)
		{
			if(m[i]>container)
			{
				container=m[i];
			}
		}
		   System.out.print("the maximum value is "+container);
	}
	}
	
           
